npts 40 40 40                        # num.grid points in xyz
gridfld 3K2Q.maps.fld                # grid_data_file
spacing 0.375                        # spacing(A)
receptor_types A C H HD N NA Na OA SA # receptor atom types
ligand_types A H HD OA               # ligand atom types
receptor 3K2Q.pdbqt                  # macromolecule
gridcenter -7.32 22.766 16.938       # xyz-coordinates or auto
smooth 0.5                           # store minimum energy w/in rad(A)
map 3K2Q.A.map                       # atom-specific affinity map
map 3K2Q.H.map                       # atom-specific affinity map
map 3K2Q.HD.map                      # atom-specific affinity map
map 3K2Q.OA.map                      # atom-specific affinity map
elecmap 3K2Q.e.map                   # electrostatic potential map
dsolvmap 3K2Q.d.map              # desolvation potential map
dielectric -0.1465                   # <0, AD4 distance-dep.diel;>0, constant
